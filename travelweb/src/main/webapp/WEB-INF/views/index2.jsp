	 				   		}else{
	 				   			viewOn = true;
	 				   		}
	 				   	}
	 				   ,error: function(result) {
	 						console.log(result);
	 				}
    			});
	 		}

	 		if(!viewOn) {
	 			$('#recentlyViewfloating > a').addClass('disabled');
	 		}

    	}


 	cntCheck();
	</script>

