					var popupX = (window.screen.width / 2) - (380 / 2);
					var popupY = (window.screen.height / 2) - (600 / 2);
					window.open('https://chat.ybtour.co.kr/ybChat.do?projectName=yb_package&entryPath=CHAT0001', 'chat', 'scrollbars=yes, resizable=no, left='+ popupX +', top='+ popupY +', width=380, height=600');
